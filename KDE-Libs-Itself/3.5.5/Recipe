url="ftp://ftp.oregonstate.edu/pub/kde/stable/3.5.5/src/kdelibs-3.5.5.tar.bz2"
recipe_type=configure
dir='kdelibs-3.5.5'

configure_options=(
	"--disable-debug"
	"--disable-dependency-tracking"
	"--disable-warnings"
	"--with-alsa"
	"--enable-fast-malloc=no"
	"--with-distribution"
	"--enable-dnotify"
	"--enable-inotify"
        "--disable-libsuffix"
#  "DO_NOT_COMPILE="
