# To succesfully compile K3B you must use Qt 3.3.X and not Qt 4.X
compile_version=1.9.0
url="$httpSourceforge/k3b/k3b-1.0.5.tar.bz2"
file_size=5113060
file_md5=e3b37d0d009af3dd149215d6ae0d54f3
recipe_type=configure

configure_options=(
    "--enable-libsuffix=none"
)

pre_build() {
   GrepReplace "\$dir/moc" "\$dir/moc-qt3" configure
   GrepReplace "\$dir/uic" "\$dir/uic-qt3" configure
}
