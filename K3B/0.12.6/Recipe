# Recipe for version 0.12.6 by Andre Detsch, on Mon Oct 31 10:01:41 BRST 2005
# To succesfully compile K3B you must use Qt 3.3.X and not Qt 4.X

pre_build() {
	echo "**"
	echo "**Plase note that you must compile K3B against Qt 3.3.X and not Qt 4.x**"
	echo "**"
	sleep 3
}

compile_version=1.7.1
url="$httpSourceforge/k3b/k3b-0.12.6.tar.bz2"
recipe_type=configure
