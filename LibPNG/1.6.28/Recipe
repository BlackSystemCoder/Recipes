# Recipe for version 1.6.28 by Wilson Moura <wilsonmourati@gmail.com>, on qui fev 16 12:41:09 BRT 2017
compile_version=1.8.5
url="ftp://ftp.simplesystems.org/pub/png/src/libpng16/libpng-1.6.28.tar.gz"
file_size=1483231
file_md5=897ccec1ebfb0922e83c2bfaa1be8748
recipe_type=configure

pre_link() {
    ln -sf libpng16 $target/include/libpng
    ln -sf libpng.so $target/lib/libpng.so.0
}
